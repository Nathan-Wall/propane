{
  "name": "propanejs",
  "version": "1.0.0",
  "description": "Materialized types for TypeScript",
  "type": "module",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node --loader ./tests/ts-loader.mjs tests/run-tests.ts",
    "lint": "eslint .",
    "propane:compile": "node scripts/propane-compiler.js",
    "runtime:build": "npm --prefix runtime run build",
    "runtime:publish": "node scripts/publish-runtime.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nathan-Wall/propane.git"
  },
  "keywords": [
    "propane",
    "propane.js",
    "propanejs"
  ],
  "author": "Nathan Wall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nathan-Wall/propane/issues"
  },
  "homepage": "https://github.com/Nathan-Wall/propane#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
