{
  "name": "propanejs",
  "version": "1.1.0",
  "description": "Materialized types for TypeScript",
  "type": "module",
  "main": "dist/runtime/index.js",
  "types": "dist/runtime/index.d.ts",
  "bin": {
    "propanec": "bin/propanec.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "node scripts/build-dist.js",
    "test": "npm run build:babel && node scripts/build-fixtures.js && node build/tests/run-tests.js",
    "lint": "eslint .",
    "build:babel": "node scripts/build-babel.js",
    "propane:compile": "npm run build:babel && node scripts/propane-compiler.js",
    "runtime:build": "npm --prefix runtime run build",
    "runtime:publish": "node scripts/publish-runtime.js",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nathan-Wall/propane.git"
  },
  "keywords": [
    "propane",
    "propane.js",
    "propanejs"
  ],
  "author": "Nathan Wall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nathan-Wall/propane/issues"
  },
  "homepage": "https://github.com/Nathan-Wall/propane#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
