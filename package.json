{
  "name": "propanejs-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "runtime",
    "pms-core",
    "pms-server",
    "pms-client",
    "tools/babel/messages",
    "tools/parser",
    "cli",
    "react",
    "examples/react-todo"
  ],
  "scripts": {
    "build": "tsc && tsc-alias && node scripts/post-build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "bundle": "node scripts/bundle.mjs",
    "test": "node scripts/build-fixtures.js && node --test 'build/**/*.test.js'",
    "lint": "eslint .",
    "propane:compile": "node scripts/propane-compiler.js",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "publish": "npm run build && node scripts/publish-all.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nathan-Wall/propane.git"
  },
  "author": "Nathan Wall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nathan-Wall/propane/issues"
  },
  "homepage": "https://github.com/Nathan-Wall/propane#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^22.10.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "react": "^18.2.0",
    "react-test-renderer": "^18.3.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
