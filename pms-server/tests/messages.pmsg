import { Endpoint } from '@propanejs/pms-core';

// @message
export type EchoRequest = Endpoint<{
  message: string,
}, EchoResponse>;

// @message
export type EchoResponse = {
  echo: string,
  timestamp: number,
};

// @message
export type AddRequest = Endpoint<{
  a: number,
  b: number,
}, AddResponse>;

// @message
export type AddResponse = {
  sum: number,
};
