import { Message } from '@propanejs/runtime';

export type Cat = Message<{
  '1:name': string;
  '2:meows': boolean;
}>;

export type Dog = Message<{
  '1:name': string;
  '2:barks': boolean;
}>;

export type PetOwner = Message<{
  '1:ownerName': string;
  '2:pet': Cat | Dog;
  '3:optionalPet'?: Cat | Dog;
}>;
