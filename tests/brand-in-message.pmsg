/**
 * Test Brand types used as properties in Message types.
 */

import type { Brand } from '@propane/runtime';
import { Message } from '@propane/runtime';

// A standalone Brand type (should transform)
export type UserId = Brand<number, 'userId'>;

// Message type with Brand property (should the nested Brand also transform?)
export type User = Message<{
  '1:id': Brand<number, 'userId'>;  // Nested Brand
  '2:name': string;
  '3:ref': UserId;  // Reference to existing Brand type
}>;
