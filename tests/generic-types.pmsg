/**
 * Test file for generic message types.
 */

// Basic item type for testing
// @message
export type Item = {
  '1:id': number;
  '2:name': string;
};

// Simple generic container with single type parameter
// @message
export type Container<T extends Message> = {
  '1:inner': T;
};

// Generic with optional field
// @message
export type Optional<T extends Message> = {
  '1:value'?: T;
};

// Multiple type parameters
// @message
export type Pair<T extends Message, U extends Message> = {
  '1:first': T;
  '2:second': U;
};

// Non-generic type that can contain generic instances
// @message
export type Parent = {
  '1:name': string;
};
